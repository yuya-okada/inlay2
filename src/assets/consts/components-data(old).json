          ], [
            {
              "type": "headline",
              "size": 100,
              "options": {
                "text": "枠線",
                "marginTop": 15
              }
            }
          ], [
            {
              "type": "color_icon",
              "size": 30,
              "options": {
                "icon": "format_color_fill",
                "defaultValue": "#000000",
                "result": "borderColor"
              }
            }
          ], [
            {
              "type": "slider",
              "size": 100,
              "options": {
                "defaultValue": 0,
                "min": 0,
                "max": 20,
                "step": 1,
                "result": "borderWidth"
