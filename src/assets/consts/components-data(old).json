          ], [
            {
              "type": "text",
              "size": 20,
              "options": {
                "text": "ぼかし"
              }
            }, {
              "type": "slider",
              "size": 80,
              "options": {
                "defaultValue": 5,
                "min": 0,
                "max": 80,
                "step": 1,
                "result": "shadowGradation"
